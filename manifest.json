{
    "manifest_version": 2 ,
    "name": "HighLighter",
    "description": "This extension will highlight the statement where the cursor resides",
    "version": "0.1",
    "browser_action": {
      "default_icon": "download.png"
    },

    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.3.1.min.js","content.js"]
        }
      ]
  }
  